#include <iostream>
using namespace std;

void Sort(int *p)
{
int a;
for(int i=0; i<9; i++)
for(int j=i+1; j<10; j++)

if (p[i] < p[j]) 
{
a=p[i];
p[i] = p[j];
p[j] = a;
}

for(int i=0; i<10; i++)
cout<<p[i]<<' ';
cout<<endl;
return;
}
int main(){
int a[10] = {1, 8, 3, 6, 5, 4, 9, 2, 8, 12};
Sort(a);
}
