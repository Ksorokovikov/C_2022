#include <iostream>
#include <math.h>
using namespace std;
 
double SquaredSum(double x, double y) {
    return pow((x + y), 2);
}

int main() {
double x, y, a, b;
cout << "Введите числа" << endl;
cin >> x >> y;
cin >> a >> b;
    
double s,t;
s = x / y;
t = a / b;
cout << SquaredSum(s, t) << endl;
   
return 0;
}
